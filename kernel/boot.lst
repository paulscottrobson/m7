814D: 219F81  			ld 		hl,__kr_no_shift_table 				; firstly identify shift state.
              	
8150: 0EFE    			ld 		c,$FE 								; check CAPS SHIFT (emulator : left shift)
8152: 06FE    			ld 		b,$FE
8154: ED78    			in 		a,(c)
8156: CB47    			bit 	0,a
8158: 2005    			jr 		nz,__kr1
815A: 21C781  			ld 		hl,__kr_shift_table
815D: 180B    			jr 		__kr2
