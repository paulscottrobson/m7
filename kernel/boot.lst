              	;
              	;		work out attribute position
              	;
824D: E5      			push 	hl 									; save position.
824E: 7C      			ld 		a,h
824F: C658    			add 	$58
8251: 67      			ld 		h,a
              	
8252: 7A      			ld 		a,d 								; get current colour
8253: E607    			and 	7  									; mask 0..2
8255: F640    			or 		$40  								; make bright
8257: 77      			ld 		(hl),a 								; store it.	
