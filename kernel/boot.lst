8186: 23      			inc 	hl
8187: 23      			inc 	hl
8188: 23      			inc 	hl
              	
8189: 7B      			ld 		a,e 								; get pattern
818A: 87      			add 	a,a 								; shift left
818B: F601    			or 		1 									; set bit 1.
818D: 5F      			ld 		e,a
              	
